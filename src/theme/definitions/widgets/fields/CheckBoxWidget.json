{
  "name": "CheckBoxWidget",
  "title": "Checkbox specific variables",
  "description": "",
  "type": "group",
  "contents": [{
      "name": "gbc-CheckBoxWidget-margin-ratio",
      "description": "",
      "type": "number",
      "defaultValue": "$theme-margin-ratio"
    },
    {
      "name": "gbc-CheckBoxWidget-checkbox-size",
      "description": "",
      "type": "size",
      "defaultValue": "$theme-checkbox-size"
    },
    {
      "name": "gbc-CheckBoxWidget-unchecked-color",
      "description": "",
      "type": "color",
      "defaultValue": "$theme-secondary-color"
    },
    {
      "name": "gbc-CheckBoxWidget-checked-color",
      "description": "",
      "type": "color",
      "defaultValue": "$theme-primary-background-color"
    },
    {
      "name": "gbc-CheckBoxWidget-unchecked-icon",
      "description": "",
      "type": "string",
      "defaultValue": "checkbox-blank-outline"
    },
    {
      "name": "gbc-CheckBoxWidget-indeterminate-icon",
      "description": "",
      "type": "string",
      "defaultValue": "minus-box"
    },
    {
      "name": "gbc-CheckBoxWidget-checked-icon",
      "description": "",
      "type": "string",
      "defaultValue": "checkbox-marked"
    },
    {
      "name": "gbc-CheckBoxWidget-disabled-color",
      "description": "",
      "type": "color",
      "defaultValue": "$theme-disabled-color"
    },
    {
      "name": "gbc-CheckBoxWidget-focus-border-color",
      "description": "",
      "type": "color",
      "defaultValue": "$mt-field-focus-border-color"
    },
    {
      "name": "gbc-ToggleCheckBoxWidget-checked-color",
      "description": "Color of a checked ToggleCheckbox",
      "type": "color",
      "defaultValue": "$theme-secondary-color"
    },
    {
      "name": "gbc-ToggleCheckBoxWidget-unchecked-color",
      "description": "Color of an unchecked ToggleCheckbox",
      "type": "color",
      "defaultValue": "rgba(0, 0, 0, 0.38)"
    },
    {
      "name": "gbc-ToggleCheckBoxWidget-disabled-color",
      "description": "Color of a disabled ToggleCheckbox",
      "type": "color",
      "defaultValue": "$palette-disabled-400"
    }
  ]
}
